<Page x:Class="Notenverwaltung.EditSubs"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Notenverwaltung"
      mc:Ignorable="d"
      Background="Transparent"
      d:DesignHeight="375"
      d:DesignWidth="560"
      Title="EditSubs"
      Loaded="Page_Loaded">

  <Border CornerRadius="5" Background="Transparent">
    
    <Grid>
      
      <ListBox x:Name="lbxSubs"
               BorderBrush="Black"
               BorderThickness="1,1,1,1"
               Style="{DynamicResource LBS}"
               Background="White"
               MouseDoubleClick="LbxSubs_MouseDoubleClick"/>

    </Grid>
    
  </Border>
  
</Page>
