<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows10.0.22000.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
    <StartupObject>Notenverwaltung.App</StartupObject>
    <SupportedOSPlatformVersion>10.0.17763.0</SupportedOSPlatformVersion>
    <Configurations>Debug;Release</Configurations>
    <Platforms>AnyCPU;x64;x86</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Datenverarbeitung\**" />
    <EmbeddedResource Remove="Datenverarbeitung\**" />
    <None Remove="Datenverarbeitung\**" />
    <Page Remove="Datenverarbeitung\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="UI\noten.png" />
    <None Remove="UI\password.png" />
    <None Remove="UI\username.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MySql.Data" Version="8.0.28" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="UI\noten.png" />
    <Resource Include="UI\password.png" />
    <Resource Include="UI\username.png" />
  </ItemGroup>

</Project>
